{
  "name": "multilingual-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en next dev -p 3000",
    "dev:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi next dev -p 3001",
    "dev:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn next dev -p 3002",
    "dev:all": "concurrently \"npm run dev:te\" \"npm run dev:hi\" \"npm run dev:kn\"",
    "build": "next build",
    "build:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en next build",
    "build:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi next build",
    "build:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn next build",
    "build:all": "npm run build:te && npm run build:hi && npm run build:kn",
    "start": "next start -p 3000",
    "start:te": "cross-env DEFAULT_LOCALE=te SUPPORTED_LOCALES=te,en next start -p 3000",
    "start:hi": "cross-env DEFAULT_LOCALE=hi SUPPORTED_LOCALES=hi next start -p 3001",
    "start:kn": "cross-env DEFAULT_LOCALE=kn SUPPORTED_LOCALES=kn next start -p 3002",
    "start:all": "concurrently \"npm run start:te\" \"npm run start:hi\" \"npm run start:kn\"",
    "lint": "next lint"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.4.0-canary.51",
    "typescript": "^5"
  }
}
